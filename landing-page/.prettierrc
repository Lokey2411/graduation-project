{
	"tabWidth": 2,
	"printWidth": 120,
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"semi": true,
	"jsxSingleQuote": true,
	"htmlWhitespaceSensitivity": "ignore",
	"quoteProps": "consistent",
	"proseWrap": "preserve",
	"bracketSameLine": true
}
